{
  "name": "@sinbad/mf-sample-workspace",
  "scripts": {
    "start": "webpack serve --port 1103",
    "start:standalone": "webpack serve --env standalone",
    "build": "webpack --mode=production",
    "analyze": "webpack --mode=production --env analyze",
    "lint": "eslint src --ext js,ts,tsx",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "test": "cross-env BABEL_ENV=test jest",
    "watch-tests": "cross-env BABEL_ENV=test jest --watch",
    "prepare": "husky install",
    "coverage": "cross-env BABEL_ENV=test jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && concurrently npm:lint"
    }
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "7.15.8",
    "@babel/eslint-parser": "7.15.8",
    "@babel/plugin-transform-runtime": "7.15.8",
    "@babel/preset-env": "7.15.8",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.15.0",
    "@babel/runtime": "7.15.4",
    "@sinbad/mf-types": "0.0.65",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "12.1.2",
    "@types/jest": "27.0.2",
    "@types/react": "17.0.32",
    "@types/react-dom": "17.0.10",
    "@types/react-redux": "7.1.20",
    "@types/react-router-dom": "5.3.1",
    "@types/systemjs": "6.1.1",
    "@types/testing-library__jest-dom": "5.14.1",
    "@types/webpack-env": "1.16.3",
    "babel-jest": "27.3.1",
    "concurrently": "6.3.0",
    "cross-env": "7.0.3",
    "eslint": "8.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-ts-react-important-stuff": "3.0.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.3.1",
    "jest-cli": "27.3.1",
    "prettier": "2.4.1",
    "pretty-quick": "3.1.1",
    "single-spa-react": "4.4.0",
    "ts-config-single-spa": "3.0.0",
    "typescript": "4.4.4",
    "webpack": "5.59.1",
    "webpack-cli": "4.9.1",
    "webpack-config-single-spa-react": "4.0.2",
    "webpack-config-single-spa-react-ts": "4.0.2",
    "webpack-config-single-spa-ts": "4.1.1",
    "webpack-dev-server": "4.3.1",
    "webpack-merge": "5.8.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "1.6.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.5",
    "react-router-dom": "5.3.0"
  }
}
